using Cadl.Http;

@route("/locks")
namespace LocksResource {
  @operationId("GetLock")
  @get
  op read(
    ...ServiceParams,
    ...SyncTokenHeader,
    @path key: string,
    @query label: string,
    ...ETagHeaders
  ): OkResponse<SyncTokenHeader & KeyValue> | Error;

  @operationId("DeleteLock")
  @delete
  op delete(
    ...ServiceParams,
    ...SyncTokenHeader,
    @path key: string,
    @query label: string,
    ...ETagHeaders
  ): OkResponse<SyncTokenHeader & KeyValue> | Error;
}
