
model Person {
  @v("read") id: string;
  @v("write") secret: string;
  name: string;
  test: string;
}

@wv("read")
model Readable<T> {
  ... T;
}

@wv("write")
model Writable<T> {
  ... T;
}

model ReadablePerson {
  ... Readable<Person>;
}

model WritablePerson {
  ... Writable<Person>;
}

@resource("/hello")
namespace Hello {
  @get op read(@path id: string, @query fieldMask: string[]): ReadablePerson;
  @post op create(person: WritablePerson): ReadablePerson;
}
