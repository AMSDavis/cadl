{
  "compilerOptions": {
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
  "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "sourceMap": true,

    "experimentalDecorators": true,
    "target": "es2019",
    "noLib": false,
    "outDir": "dist",
    "rootDir": ".",
    "declaration": true,
    "declarationMap": false,
    "removeComments": false,
    "lib": [
      "es2019"
    ],
    "stripInternal": true,
    "types": [
      "node"
    ],
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "dist/**",
    "resources",
    "node_modules",
  ]
}